<h2>Assay {{ ident }}</h2>

<h3>Metadata</h3>
<table>
  {% for row in metadata %}
  <tr>
	{% for val in row %}
	<td>{{ val }}</td>
	{% endfor %}
  </tr>
  {% endfor %}
</table>

<h3>Treatments</h3>
<table>
  {% for row in treatments %}
  <tr>
	{% for val in row %}
	<td>{{ val }}</td>
	{% endfor %}
  </tr>
  {% endfor %}
</table>

<h3>Readings</h3>
<table>
  {% for row in readings %}
  <tr>
	{% for val in row %}
	<td>{{ val }}</td>
	{% endfor %}
  </tr>
  {% endfor %}
</table>
