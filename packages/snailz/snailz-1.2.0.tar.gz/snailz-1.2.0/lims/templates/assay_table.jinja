<table>
  <tr>
    {% for h in ["assay ID", "date", "specimen ID", "person ID", "machine ID"] %}
    <th>{{ h }}</th>
    {% endfor %}
  </tr>
  {% for row in assays %}
  <tr>
    <td><a href="#" hx-trigger="click" hx-get="/assay/{{ row['ident'] }}" hx-target="#details">{{ row["ident"] }}</a></td>
    <td>{{ row["performed"] }}</td>
    <td>{{ row["specimen"] }}</td>
    <td><a href="#" hx-trigger="click" hx-get="/person/{{ row['person'] }}" hx-target="#details">{{ row["person"] }}</a></td>
    <td><a href="#" hx-trigger="click" hx-get="/machine/{{ row['machine'] }}" hx-target="#details">{{ row["machine"] }}</a></td>
  </tr>
  {% endfor %}
</table>
