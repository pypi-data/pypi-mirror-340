<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <data>
    <record id="view_form_crm_lead_generate_SIM_delivery_wizard" model="ir.ui.view">
      <field name="name">CRM Lead Generate SIM delivery Wizard</field>
      <field name="model">crm.lead.generate.sim.delivery.wizard</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <p>Generate SIM delivery with correos with these CRM Leads?</p>
          </sheet>
          <footer>
            <button type="object" name="button_generate_delivery" string="Continua" />
            <button special="cancel" string="Cancel" class="btn-secondary"/>
          </footer>
        </form>
      </field>
    </record>
    <act_window id="action_crm_lead_generate_SIM_delivery_wizard"
                name="Generate SIM delivery"
                src_model="crm.lead"
                res_model="crm.lead.generate.sim.delivery.wizard"
                view_type="form"
                view_mode="form"
                target="new"
                multi="True"
                />
  </data>
</odoo>
