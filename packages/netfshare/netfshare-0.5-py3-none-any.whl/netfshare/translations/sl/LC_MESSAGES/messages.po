#: netfshare/netfshare.py:396
#, python-format
msgid "Too many files. Max. %(num_files)d files per upload."
msgstr ""
"Preveč datotek izbranih. Dovoljeno nalaganje največ %(num_files)d datotek "
"naenkrat."

#: netfshare/netfshare.py:401
msgid "An upload with the same ID already exists."
msgstr "Datoteke so že bile oddane."

#: netfshare/netfshare.py:404
msgid ""
"An upload with the same ID already exists. Files with matching names were "
"overwritten."
msgstr "Datoteke so že bile oddane. Tiste, z enakimi imeni, so bile prepisane."

#: netfshare/netfshare.py:424
#, python-format
msgid "%(num_files)d files successfully uploaded."
msgstr "%(num_files)d datotek uspešno oddanih."

