# 浏览器配置
MAX_BROWSER_INSTANCES=2

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# 图片处理配置
MAX_CONCURRENT_DOWNLOADS=5
MIN_IMAGE_SIZE=1024
MAX_IMAGE_SIZE=10485760

# Elasticsearch配置
ES_HOST=localhost
ES_PORT=9200
ES_INDEX=clipsight
ES_MIN_SCORE=0.5

# MinIO配置
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=clipsight
MINIO_SECURE=false

# 代理配置
PROXY_ENABLED=true
PROXY_URL=http://127.0.0.1:1080

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=clip-sight.log

# Web 服务配置
WEB_HOST=0.0.0.0
WEB_PORT=8000
DEBUG=false

# Worker 配置
WORKER_HOST=0.0.0.0
WORKER_PORT=8001
MAX_WORKERS=4
MAX_BROWSERS=2
MAX_RETRIES=3
RETRY_DELAY=5

# 任务配置
MAX_IMAGES_PER_TASK=20
TASK_TIMEOUT=3600
TASK_PRIORITY=1 