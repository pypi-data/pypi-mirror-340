{
	"template": "<YEAR>.<MONTH>.<COUNTER>",
	"version": "2025.4.8",
	"YEAR": 2025,
	"MONTH": 4,
	"COUNTER": 8,
	"targets": {
		"pyproject.toml": {
			"find": "\"<VERSION>\"\\s*#\\s*vinc",
			"replace": "\"<VERSION>\" # vinc"
		},
		"auto_dlp/version.py": {
			"find": ".+",
			"replace": "program_version = \"<VERSION>\""
		}
	}
}